class MyQueue {
public:
    int tail, head;
    vector<int> queue;
    MyQueue(int K_MAX) {//max_length of queue
        head = 0;
        tail = -1;
        queue.assign(K_MAX, 0);
    }
    void push(int x) {
        tail = (tail+1)%3;
        queue[tail] = x;
    }
    int pop() {
        int t = head;
        head = (head+1)%3;
        return queue[t];
    }
    int peek() {
        return queue[head];
    }
    bool empty() {
        return tail == head-1;
    }
};
